
<h1 class="table tablaEmpresasAInvitar">EMPRESAS A INVITAR EXP. NUMERO: {{nroExp}}.</h1>
<table class="table tablaEmpresasAInvitar">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Nombre</th>
      <th scope="col">Correo</th>
      <th scope="col">Telefono</th>
      <th scope="col">Contacto</th>
      <th scope="col">Eliminar</th>
    </tr>
  </thead>
  <tbody>
      {{#each empresas }}
    <tr>
      <th>{{nombre}}</th>
      <td>{{correo}}</td>
      <td>{{telefono}}</td>
      <td>{{contacto}}</td>
      <td> <button class="btn btn-danger" onclick="eliminar('{{nombre}}')">Eliminar</button></td>
    </tr>
        {{/each}}
  </tbody>
</table>

<input type="hidden" value="{{nroExp}}" id="expediente">

<nav class="navbar navbar-light bg-light tablaEmpresasAInvitar">
  <div class="ui-widget">
  <form class="form-inline"  action="/agregar" method="POST">
    <input class="form-control mr-sm-2" type="search" placeholder="Empresa" aria-label="Search" autocomplete="off" name="empresa" id="agregaEmpresa">
    <input type="hidden" value="{{nroExp}}" name="nroExp">
    <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Agregar</button>
  </form>
  </div>
</nav>
<button>Terminar Listado</button>

<script src="../../javascripts/jquery/jquery.js"></script>
<script src="../../javascripts/jquery/jquery-ui.js"></script>
<script src="../javascripts/vistas/modificarJS.js"></script>