<div class="container box">
{{#if altaOk}}
{{!-- <div class="alert alert-success" role="alert">
  {{altaOk}}
</div> --}}
{{else}}
<form  action="/alta" method="POST" id="formuAlta" onsubmit='return validar()'>
	<div class="form-group">
		<label for="idNombre">Nombre o Razon social:</label>
		<input type="text" name="nombre" class="form-control" id="idNombre" required autocomplete="off">
	</div>
	<div class="form-group">
		<label for="idCorreo">Correo Electronico:</label>
		<input type="mail" name="correo" class="form-control" id="idCorreo" required autocomplete="off">
	</div>
	<div class="form-group">
		<label for="idTel">Telefono:</label>
		<input type="text" name="telefono" placeholder="4346-7300 int 7722" class="form-control" id="idTel" required autocomplete="off">
	</div>
	<div class="form-group">
		<label for="idContacto">Contacto:</label>
		<input type="text" name="contacto" placeholder="Vendedor, o persona de contacto" class="form-control" id="idContacto" autocomplete="off">
	</div>
	<div class="form-group">
		<label for="irRamo">Ramo que vende:</label>
		<input type="text" name="ramo" class="form-control" placeholder="Si son varios, separar por comas."id="idRamo" autocomplete="off">
	</div>
		<input type="submit" name="Cargar" value="Cargar Proveedor" class="btn btn-primary" id="botonAlta">
</form>
{{/if}}
</div>

<script src="../../javascripts/jquery/jquery.js"></script>
<script src="../javascripts/vistas/altaJS.js"></script>
