<div class="container box">
  {{#if mensaje}}
  <div class="alert alert-danger" role="alert">
    {{mensaje}}, intente nuevamente.
  </div>
  {{/if}}
  <nav class="navbar navbar-light bg-light">
    <a class="navbar-brand">Crear listado</a>
    <form class="form-inline ui-widget" action="/armar" method="POST" id="formCrearExp">
      <label for="nroExp">Nro de Expediente: </label>
      <input class="form-control mr-sm-2" type="search" name="nroExp" aria-label="Search" id="nroExp" autocomplete="off"> 
      <label for="ramo">Ramo: </label>
      <input class="form-control mr-sm-2" type="search" name="ramo" aria-label="Search" id="ramo" autocomplete="off">
      <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Crear</button>
    </form>
  </nav>
</div>

<script src="../../javascripts/jquery/jquery.js"></script>
<script src="../../javascripts/jquery/jquery-ui.js"></script>
<script src="../javascripts/vistas/ver-js.js"></script>